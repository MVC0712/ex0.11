{
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation":false
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "prettier.trailingComma": "all",
  "prettier.useEditorConfig": false,
  "prettier.useTabs": true,
  "cSpell.words": [
    "colspan"
  ]
}